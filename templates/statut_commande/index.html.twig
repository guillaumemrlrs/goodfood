{% extends 'base.html.twig' %}

{% block title %}StatutCommande index{% endblock %}

{% block body %}
    <h1>StatutCommande index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>IdStatutCommande</th>
                <th>LibelleStatutCommande</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for statut_commande in statut_commandes %}
            <tr>
                <td>{{ statut_commande.idStatutCommande }}</td>
                <td>{{ statut_commande.libelleStatutCommande }}</td>
                <td>
                    <a href="{{ path('statut_commande_show', {'idStatutCommande': statut_commande.idStatutCommande}) }}">show</a>
                    <a href="{{ path('statut_commande_edit', {'idStatutCommande': statut_commande.idStatutCommande}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('statut_commande_new') }}">Create new</a>
{% endblock %}
